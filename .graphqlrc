schema: "./introspection/schema.graphql"
extensions:
  graphql-markdown:
    rootPath: ./docs
    baseURL: "/"
    homepage: "static/index.md"
    loaders:
      SchemaLoader: "@graphql-tools/graphql-file-loader"
    docOptions:
      frontMatter:
        pagination_next: null
        pagination_prev: null
    printTypeOptions:
      deprecated: "group"
